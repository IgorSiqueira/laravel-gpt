{
    "name": "igorsiqueira/laravel-gpt",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "description": "Um pacote para integrar seu projeto Laravel ao OpenIa com objetivo de gerar classes mais robustas",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Siqueira",
            "email": "igor-ti@outlook.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^10.0"
        
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "IgorSiqueira\\LaravelGPT\\": "src/",
            "IgorSiqueira\\LaravelGPT\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IgorSiqueira\\LaravelGPT\\Providers\\LaravelGPTServiceProvider"
            ]
        }
    }
}
